generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserProfile {
  id         String      @id @default(cuid())
  email      String      @unique
  createdAt  DateTime    @default(now())
  role       UserRole?
  activeStatus Boolean     @default(true)
  password   String

}


enum UserRole {
  ADMIN
  PARENT
  SERVICE
  COMPETITOR
}